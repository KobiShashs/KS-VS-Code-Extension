{
		"showLoading": {
		"prefix": "ks-html-loading",
		"body": [
			"<img *ngIf=\"$1\" src=\"/assets/$2\" >"

		],
		"description": "show loading structural directive"
		},

		"showTable": {
			"prefix": "ks-html-table",
			"body": [
				"<div *ngIf=\"products\">",
    "<table>",
        "<tr>",
            "<th>Id</th>",
            "<th>Name</th>",
            "<th>Price</th>",
            "<th>Amount</th>",
        "</tr>",
        "<tr *ngFor=\"let p of products\">",
            "<td>{{p.id}}</td>",
            "<td>{{p.name}}</td>",
            "<td>{{p.price}}</td>",
            "<td>{{p.amount}}</td>",
        "</tr>",
    "</table>",
"</div>"
			],
			"description": "show loading structural directive"
			},

